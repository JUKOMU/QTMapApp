ko*kv+kq*-kt+kn*-ku-kp*-kr;ks[2]=kn*kv+kq*-kr+kp*-kt-ko*-ku;return ks};ki.forEach=(function(){var km=ki.create();return function(kp,kt,ku,ks,kr,kn){var kq,ko;if(!kt){kt=4}if(!ku){ku=0}if(ks){ko=Math.min((ks*kt)+ku,kp.length)}else{ko=kp.length}for(kq=ku;kq<ko;kq+=kt){km[0]=kp[kq];km[1]=kp[kq+1];km[2]=kp[kq+2];km[3]=kp[kq+3];kr(km,km,kn);kp[kq]=km[0];kp[kq+1]=km[1];kp[kq+2]=km[2];kp[kq+3]=km[3]}return kp}})();ki.str=function(km){return"vec4("+km[0]+", "+km[1]+", "+km[2]+", "+km[3]+")"};kh.vec4=ki;var ke={};ke.create=function(kn){kn=kn||i;var km=new kn(4);km[0]=1;km[1]=0;km[2]=0;km[3]=1;return km};ke.clone=function(km,ko){ko=ko||i;var kn=new ko(4);kn[0]=km[0];kn[1]=km[1];kn[2]=km[2];kn[3]=km[3];return kn};ke.copy=function(kn,km){kn[0]=km[0];kn[1]=km[1];kn[2]=km[2];kn[3]=km[3];return kn};ke.identity=function(km){km[0]=1;km[1]=0;km[2]=0;km[3]=1;return km};ke.transpose=function(ko,kn){if(ko===kn){var km=kn[1];ko[1]=kn[2];ko[2]=km}else{ko[0]=kn[0];ko[1]=kn[2];ko[2]=kn[1];ko[3]=kn[3]}return ko};ke.invert=function(kq,ko){var kp=ko[0],kn=ko[1],km=ko[2],ks=ko[3],kr=kp*ks-km*kn;if(!kr){return null}kr=1/kr;kq[0]=ks*kr;kq[1]=-kn*kr;kq[2]=-km*kr;kq[3]=kp*kr;return kq};ke.adjoint=function(ko,km){var kn=km[0];ko[0]=km[3];ko[1]=-km[1];ko[2]=-km[2];ko[3]=kn;return ko};ke.determinant=function(km){return km[0]*km[3]-km[2]*km[1]};ke.multiply=function(kq,kv,kt){var kp=kv[0],ko=kv[1],kn=kv[2],km=kv[3];var kw=kt[0],ku=kt[1],ks=kt[2],kr=kt[3];kq[0]=kp*kw+kn*ku;kq[1]=ko*kw+km*ku;kq[2]=kp*ks+kn*kr;kq[3]=ko*ks+km*kr;return kq};ke.mul=ke.multiply;ke.rotate=function(kq,kt,ks){var kp=kt[0],ko=kt[1],kn=kt[2],km=kt[3],ku=Math.sin(ks),kr=Math.cos(ks);kq[0]=kp*kr+kn*ku;kq[1]=ko*kr+km*ku;kq[2]=kp*-ku+kn*kr;kq[3]=ko*-ku+km*kr;return kq};ke.scale=function(kq,kr,kt){var kp=kr[0],ko=kr[1],kn=kr[2],km=kr[3],ku=kt[0],ks=kt[1];kq[0]=kp*ku;kq[1]=ko*ku;kq[2]=kn*ks;kq[3]=km*ks;return kq};ke.str=function(km){return"mat2("+km[0]+", "+km[1]+", "+km[2]+", "+km[3]+")"};ke.frob=function(km){return(Math.sqrt(Math.pow(km[0],2)+Math.pow(km[1],2)+Math.pow(km[2],2)+Math.pow(km[3],2)))};ke.LDU=function(km,kp,ko,kn){km[2]=kn[2]/kn[0];ko[0]=kn[0];ko[1]=kn[1];ko[3]=kn[3]-km[2]*ko[1];return[km,kp,ko]};kh.mat2=ke;var e={};e.create=function(kn){kn=kn||i;var km=new kn(16);km[0]=1;km[1]=0;km[2]=0;km[3]=0;km[4]=0;km[5]=1;km[6]=0;km[7]=0;km[8]=0;km[9]=0;km[10]=1;km[11]=0;km[12]=0;km[13]=0;km[14]=0;km[15]=1;return km};e.clone=function(km){var kn=new i(16);kn[0]=km[0];kn[1]=km[1];kn[2]=km[2];kn[3]=km[3];kn[4]=km[4];kn[5]=km[5];kn[6]=km[6];kn[7]=km[7];kn[8]=km[8];kn[9]=km[9];kn[10]=km[10];kn[11]=km[11];kn[12]=km[12];kn[13]=km[13];kn[14]=km[14];kn[15]=km[15];return kn};e.copy=function(kn,km){kn[0]=km[0];kn[1]=km[1];kn[2]=km[2];kn[3]=km[3];kn[4]=km[4];kn[5]=km[5];kn[6]=km[6];kn[7]=km[7];kn[8]=km[8];kn[9]=km[9];kn[10]=km[10];kn[11]=km[11];kn[12]=km[12];kn[13]=km[13];kn[14]=km[14];kn[15]=km[15];return kn};e.identity=function(km){km[0]=1;km[1]=0;km[2]=0;km[3]=0;km[4]=0;km[5]=1;km[6]=0;km[7]=0;km[8]=0;km[9]=0;km[10]=1;km[11]=0;km[12]=0;km[13]=0;km[14]=0;km[15]=1;return km};e.transpose=function(kp,ko){if(kp===ko){var kt=ko[1],kr=ko[2],kq=ko[3],km=ko[6],ks=ko[7],kn=ko[11];kp[1]=ko[4];kp[2]=ko[8];kp[3]=ko[12];kp[4]=kt;kp[6]=ko[9];kp[7]=ko[13];kp[8]=kr;kp[9]=km;kp[11]=ko[14];kp[12]=kq;kp[13]=ks;kp[14]=kn}else{kp[0]=ko[0];kp[1]=ko[4];kp[2]=ko[8];kp[3]=ko[12];kp[4]=ko[1];kp[5]=ko[5];kp[6]=ko[9];kp[7]=ko[13];kp[8]=ko[2];kp[9]=ko[6];kp[10]=ko[10];kp[11]=ko[14];kp[12]=ko[3];kp[13]=ko[7];kp[14]=ko[11];kp[15]=ko[15]}return kp};e.invert=function(kF,kK){var kO=kK[0],kM=kK[1],kL=kK[2],kI=kK[3],kq=kK[4],kp=kK[5],ko=kK[6],kn=kK[7],kE=kK[8],kD=kK[9],kC=kK[10],kB=kK[11],kQ=kK[12],kP=kK[13],kN=kK[14],kJ=kK[15],kA=kO*kp-kM*kq,kz=kO*ko-kL*kq,ky=kO*kn-kI*kq,kx=kM*ko-kL*kp,kw=kM*kn-kI*kp,kv=kL*kn-kI*ko,ku=kE*kP-kD*kQ,kt=kE*kN-kC*kQ,ks=kE*kJ-kB*kQ,kr=kD*kN-kC*kP,kH=kD*kJ-kB*kP,kG=kC*kJ-kB*kN,km=kA*kG-kz*kH+ky*kr+kx*ks-kw*kt+kv*ku;if(!km){return null}km=1/km;kF[0]=(kp*kG-ko*kH+kn*kr)*km;kF[1]=(kL*kH-kM*kG-kI*kr)*km;kF[2]=(kP*kv-kN*kw+kJ*kx)*km;kF[3]=(kC*kw-kD*kv-kB*kx)*km;kF[4]=(ko*ks-kq*kG-kn*kt)*km;kF[5]=(kO*kG-kL*ks+kI*kt)*km;kF[6]=(kN*ky-kQ*kv-kJ*kz)*km;kF[7]=(kE*kv-kC*ky+kB*kz)*km;kF[8]=(kq*kH-kp*ks+kn*ku)*km;kF[9]=(kM*ks-kO*kH-kI*ku)*km;kF[10]=(kQ*kw-kP*ky+kJ*kA)*km;kF[11]=(kD*ky-kE*kw-kB*kA)*km;kF[12]=(kp*kt-kq*kr-ko*ku)*km;kF[13]=(kO*kr-kM*kt+kL*ku)*km;kF[14]=(kP*kz-kQ*kx-kN*kA)*km;kF[15]=(kE*kx-kD*kz+kC*kA)*km;return kF};e.adjoint=function(ku,kx){var kB=kx[0],kz=kx[1],ky=kx[2],kv=kx[3],kp=kx[4],ko=kx[5],kn=kx[6],km=kx[7],kt=kx[8],ks=kx[9],kr=kx[10],kq=kx[11],kD=kx[12],kC=kx[13],kA=kx[14],kw=kx[15];ku[0]=(ko*(kr*kw-kq*kA)-ks*(kn*kw-km*kA)+kC*(kn*kq-km*kr));ku[1]=-(kz*(kr*kw-kq*kA)-ks*(ky*kw-kv*kA)+kC*(ky*kq-kv*kr));ku[2]=(kz*(kn*kw-km*kA)-ko*(ky*kw-kv*kA)+kC*(ky*km-kv*kn));ku[3]=-(kz*(kn*kq-km*kr)-ko*(ky*kq-kv*kr)+ks*(ky*km-kv*kn));ku[4]=-(kp*(kr*kw-kq*kA)-kt*(kn*kw-km*kA)+kD*(kn*kq-km*kr));ku[5]=(kB*(kr*kw-kq*kA)-kt*(ky*kw-kv*kA)+kD*(ky*kq-kv*kr));ku[6]=-(kB*(kn*kw-km*kA)-kp*(ky*kw-kv*kA)+kD*(ky*km-kv*kn));ku[7]=(kB*(kn*kq-km*kr)-kp*(ky*kq-kv*kr)+kt*(ky*km-kv*kn));ku[8]=(kp*(ks*kw-kq*kC)-kt*(ko*kw-km*kC)+kD*(ko*kq-km*ks));ku[9]=-(kB*(ks*kw-kq*kC)-kt*(kz*kw-kv*kC)+kD*(kz*kq-kv*ks));ku[10]=(kB*(ko*kw-km*kC)-kp*(kz*kw-kv*kC)+kD*(kz*km-kv*ko));ku[11]=-(kB*(ko*kq-km*ks)-kp*(kz*kq-kv*ks)+kt*(kz*km-kv*ko));ku[12]=-(kp*(ks*kA-kr*kC)-kt*(ko*kA-kn*kC)+kD*(ko*kr-kn*ks));ku[13]=(kB*(ks*kA-kr*kC)-kt*(kz*kA-ky*kC)+kD*(kz*kr-ky*ks));ku[14]=-(kB*(ko*kA-kn*kC)-kp*(kz*kA-ky*kC)+kD*(kz*kn-ky*ko));ku[15]=(kB*(ko*kr-kn*ks)-kp*(kz*kr-ky*ks)+kt*(kz*kn-ky*ko));return ku};e.determinant=function(kH){var kM=kH[0],kK=kH[1],kI=kH[2],kG=kH[3],kp=kH[4],ko=kH[5],kn=kH[6],km=kH[7],kD=kH[8],kC=kH[9],kB=kH[10],kA=kH[11],kO=kH[12],kN=kH[13],kL=kH[14],kJ=kH[15],kz=kM*ko-kK*kp,ky=kM*kn-kI*kp,kx=kM*km-kG*kp,kw=kK*kn-kI*ko,kv=kK*km-kG*ko,ku=kI*km-kG*kn,kt=kD*kN-kC*kO,ks=kD*kL-kB*kO,kr=kD*kJ-kA*kO,kq=kC*kL-kB*kN,kF=kC*kJ-kA*kN,kE=kB*kJ-kA*kL;return kz*kE-ky*kF+kx*kq+kw*kr-kv*ks+ku*kt};e.multiply=function(ky,kC,kz){var kG=kC[0],kF=kC[1],kD=kC[2],kA=kC[3],ks=kC[4],kq=kC[5],ko=kC[6],km=kC[7],kx=kC[8],kw=kC[9],kv=kC[10],ku=kC[11],kI=kC[12],kH=kC[13],kE=kC[14],kB=kC[15];var kt=kz[0],kr=kz[1],kp=kz[2],kn=kz[3];ky[0]=kt*kG+kr*ks+kp*kx+kn*kI;ky[1]=kt*kF+kr*kq+kp*kw+kn*kH;ky[2]=kt*kD+kr*ko+kp*kv+kn*kE;ky[3]=kt*kA+kr*km+kp*ku+kn*kB;kt=kz[4];kr=kz[5];kp=kz[6];kn=kz[7];ky[4]=kt*kG+kr*ks+kp*kx+kn*kI;ky[5]=kt*kF+kr*kq+kp*kw+kn*kH;ky[6]=kt*kD+kr*ko+kp*kv+kn*kE;ky[7]=kt*kA+kr*km+kp*ku+kn*kB;kt=kz[8];kr=kz[9];kp=kz[10];kn=kz[11];ky[8]=kt*kG+kr*ks+kp*kx+kn*kI;ky[9]=kt*kF+kr*kq+kp*kw+kn*kH;ky[10]=kt*kD+kr*ko+kp*kv+kn*kE;ky[11]=kt*kA+kr*km+kp*ku+kn*kB;kt=kz[12];kr=kz[13];kp=kz[14];kn=kz[15];ky[12]=kt*kG+kr*ks+kp*kx+kn*kI;ky[13]=kt*kF+kr*kq+kp*kw+kn*kH;ky[14]=kt*kD+kr*ko+kp*kv+kn*kE;ky[15]=kt*kA+kr*km+kp*ku+kn*kB;return ky};e.mul=e.multiply;e.translate=function(ky,kA,kt){var ks=kt[0],kr=kt[1],kq=kt[2],kD,kC,kB,kz,kp,ko,kn,km,kx,kw,kv,ku;if(kA===ky){ky[12]=kA[0]*ks+kA[4]*kr+kA[8]*kq+kA[12];ky[13]=kA[1]*ks+kA[5]*kr+kA[9]*kq+kA[13];ky[14]=kA[2]*ks+kA[6]*kr+kA[10]*kq+kA[14];ky[15]=kA[3]*ks+kA[7]*kr+kA[11]*kq+kA[15]}else{kD=kA[0];kC=kA[1];kB=kA[2];kz=kA[3];kp=kA[4];ko=kA[5];kn=kA[6];km=kA[7];kx=kA[8];kw=kA[9];kv=kA[10];ku=kA[11];ky[0]=kD;ky[1]=kC;ky[2]=kB;ky[3]=kz;ky[4]=kp;ky[5]=ko;ky[6]=kn;ky[7]=km;ky[8]=kx;ky[9]=kw;ky[10]=kv;ky[11]=ku;ky[12]=kD*ks+kp*kr+kx*kq+kA[12];ky[13]=kC*ks+ko*kr+kw*kq+kA[13];ky[14]=kB*ks+kn*kr+kv*kq+kA[14];ky[15]=kz*ks+km*kr+ku*kq+kA[15]}return ky};e.scale=function(kp,kn,ko){var km=ko[0],kr=ko[1],kq=ko[2];kp[0]=kn[0]*km;kp[1]=kn[1]*km;kp[2]=kn[2]*km;kp[3]=kn[3]*km;kp[4]=kn[4]*kr;kp[5]=kn[5]*kr;kp[6]=kn[6]*kr;kp[7]=kn[7]*kr;kp[8]=kn[8]*kq;kp[9]=kn[9]*kq;kp[10]=kn[10]*kq;kp[11]=kn[11]*kq;kp[12]=kn[12];kp[13]=kn[13];kp[14]=kn[14];kp[15]=kn[15];return kp};e.rotate=function(kG,kN,kP,km){var kw=km[0],kv=km[1],ku=km[2],kH=Math.sqrt(kw*kw+kv*kv+ku*ku),kB,kL,kA,kR,kQ,kO,kM,kt,ks,kr,kq,kF,kE,kD,kC,kz,ky,kx,kK,kJ,kI,kp,ko,kn;if(Math.abs(kH)<kl){return null}kH=1/kH;kw*=kH;kv*=kH;ku*=kH;kB=Math