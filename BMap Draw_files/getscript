+kr,ku=kq+kq,kp=ks*kA,ko=ks*km,kn=ks*ku,ky=kr*km,kw=kr*ku,kD=kq*ku,kE=kt*kA,kC=kt*km,kB=kt*ku;kz[0]=1-(ky+kD);kz[1]=ko+kB;kz[2]=kn-kC;kz[3]=0;kz[4]=ko-kB;kz[5]=1-(kp+kD);kz[6]=kw+kE;kz[7]=0;kz[8]=kn+kC;kz[9]=kw-kE;kz[10]=1-(kp+ky);kz[11]=0;kz[12]=kv[0];kz[13]=kv[1];kz[14]=kv[2];kz[15]=1;return kz};e.fromQuat=function(kw,kt){var kq=kt[0],kp=kt[1],ko=kt[2],kr=kt[3],kx=kq+kq,km=kp+kp,ks=ko+ko,kn=kq*kx,kv=kp*kx,ku=kp*km,kD=ko*kx,kC=ko*km,kA=ko*ks,kB=kr*kx,kz=kr*km,ky=kr*ks;kw[0]=1-ku-kA;kw[1]=kv+ky;kw[2]=kD-kz;kw[3]=0;kw[4]=kv-ky;kw[5]=1-kn-kA;kw[6]=kC+kB;kw[7]=0;kw[8]=kD+kz;kw[9]=kC-kB;kw[10]=1-kn-ku;kw[11]=0;kw[12]=0;kw[13]=0;kw[14]=0;kw[15]=1;return kw};e.frustum=function(kq,kn,kv,km,ku,ks,kr){var kt=1/(kv-kn),kp=1/(ku-km),ko=1/(ks-kr);kq[0]=(ks*2)*kt;kq[1]=0;kq[2]=0;kq[3]=0;kq[4]=0;kq[5]=(ks*2)*kp;kq[6]=0;kq[7]=0;kq[8]=(kv+kn)*kt;kq[9]=(ku+km)*kp;kq[10]=(kr+ks)*ko;kq[11]=-1;kq[12]=0;kq[13]=0;kq[14]=(kr*ks*2)*ko;kq[15]=0;return kq};e.perspective=function(kp,ko,kn,kq,km){var ks=1/Math.tan(ko/2),kr=1/(kq-km);kp[0]=ks/kn;kp[1]=0;kp[2]=0;kp[3]=0;kp[4]=0;kp[5]=ks;kp[6]=0;kp[7]=0;kp[8]=0;kp[9]=0;kp[10]=(km+kq)*kr;kp[11]=-1;kp[12]=0;kp[13]=0;kp[14]=(2*km*kq)*kr;kp[15]=0;return kp};e.ortho=function(kp,kn,kv,km,kt,ks,kr){var kq=1/(kn-kv),ku=1/(km-kt),ko=1/(ks-kr);kp[0]=-2*kq;kp[1]=0;kp[2]=0;kp[3]=0;kp[4]=0;kp[5]=-2*ku;kp[6]=0;kp[7]=0;kp[8]=0;kp[9]=0;kp[10]=2*ko;kp[11]=0;kp[12]=(kn+kv)*kq;kp[13]=(kt+km)*ku;kp[14]=(kr+ks)*ko;kp[15]=1;return kp};e.lookAt=function(kA,kH,kI,ks){var kG,kF,kD,ko,kn,km,kv,ku,kt,kB,kE=kH[0],kC=kH[1],kz=kH[2],kr=ks[0],kq=ks[1],kp=ks[2],ky=kI[0],kx=kI[1],kw=kI[2];if(Math.abs(kE-ky)<kl&&Math.abs(kC-kx)<kl&&Math.abs(kz-kw)<kl){return e.identity(kA)}kv=kE-ky;ku=kC-kx;kt=kz-kw;kB=1/Math.sqrt(kv*kv+ku*ku+kt*kt);kv*=kB;ku*=kB;kt*=kB;kG=kq*kt-kp*ku;kF=kp*kv-kr*kt;kD=kr*ku-kq*kv;kB=Math.sqrt(kG*kG+kF*kF+kD*kD);if(!kB){kG=0;kF=0;kD=0}else{kB=1/kB;kG*=kB;kF*=kB;kD*=kB}ko=ku*kD-kt*kF;kn=kt*kG-kv*kD;km=kv*kF-ku*kG;kB=Math.sqrt(ko*ko+kn*kn+km*km);if(!kB){ko=0;kn=0;k